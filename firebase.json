{
  "hosting": {
    "public": "dist",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [{
      "source" : "/static/manifest.json",
      "headers" : [{
        "key" : "Access-Control-Allow-Origin",
        "value" : "*"
      } ]
    }]
  }
}
